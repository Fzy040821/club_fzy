<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="24dp"
                android:left="24dp"
                android:right="24dp"
                android:top="24dp" />
            <gradient
                android:angle="-90"
                android:endColor="#00CDF1FF"
                android:startColor="#947EE6FD" />
        </shape>


    </item>

    <item>
        <shape android:shape="oval">

            <solid android:color="@color/white" />
        </shape>
    </item>


</layer-list>